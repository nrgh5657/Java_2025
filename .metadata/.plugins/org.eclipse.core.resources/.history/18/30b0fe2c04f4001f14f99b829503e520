package ch02.sec05;

public class booleanEx2 {

	public static void main(String[] args) {

		boolean stop = true;
		
		stop = false;
		
		/*
		 *  >  크다
		 *  >= 이상
		 *  <  작다
		 *  <= 미만
		 *  == 같다
		 *  != 같지 않다
		 *  !  부정
		 *  
		 */
		
		boolean check = true;
		System.out.println(check);
			
		
		
	}

}
