package ch01.ex04;

public class OperatorExample {

	public static void main(String[] args) {

		int a= 5;
		int b= 2;
		
		System.out.println(a+b);
		System.out.println(a-b);
		System.out.println(a*b);
		System.out.println(a/b); // 정수/정수 정수값 출력, 소수점 이하는 버림
		
		System.out.println(a%b); // 나버지 연산자, 나머지 값을 출력
	}

}
