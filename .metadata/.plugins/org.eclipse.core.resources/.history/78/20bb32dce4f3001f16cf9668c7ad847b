package ch02.sec02;

public class CharEx {

	public static void main(String[] args) {
		
		// char => 2byte, 16bit, 0 ~ 2^16-1,  -32,768 ~ 32,767
		
		char c1 = 'A';
		char c2 = 65;
		
		char c3 = 'ê°€';
		char c4 = 44032;
		
		System.out.println(c1);
		System.out.println(c2);
		System.out.println(c3);
		System.out.println(c4);
	}

}
